== Event Manager Unit Tests ==
Tests that the EventManager class correctly dispatches
events to event subscribers.

== Event manager initializes subscription list on construct ==
-
TEST(EventManager, InitializeList) {
  EventManager em = EventManager();
  ASSERT_EQ(0, em.Subscribers("MockEvent"));
}
-

== Classes can subscribe to events ==
-
TEST(EventManager, EventSubscription) {
  EventManager em = EventManager();
  em.Subscribe("MockEvent", SomeHandler);
  ASSERT_EQ(1, em.Subscribers("MockEvent"));
}
-

== Classes can dispatch events through the manager ==
-
TEST(EventManager, EventDispatch) {
  ASSERT_FALSE(true);
}
-

== Listeners recieve events ==
-
TEST(EventManager, ListenerReceive) {
  ASSERT_FALSE(true);
}
-

